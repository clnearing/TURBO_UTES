<div class="container p-1">
  <h2>Add A Review</h2>
  <div class="btn btn-link" style="border: solid 1px"><%= link_to "Back", dashboard_path %></div>

  <%= simple_form_for @review, url: ute_booking_reviews_path(params[:ute_id], params[:booking_id]) do |f| %>
  <%= f.input :rating %>
  <%= f.input :comments %>
  <%= f.button :submit, class:"btn btn-primary"%>
  <% end %>
</div>
